{
    "version": 2,
    "builds":[
        {
            "src": "requirements.txt",
            "use": "@vercel/pip"
          },
        {
            "src":"myshop/wsgi.py",
            "use":"@vercel/python",
            "config": {
                "maxLamdasize": "15mb", 
                "runtime": "python3.9"
                     }
        },
        {
            "src": "builds.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "static-build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "http://localhost:${VERCEL_PORT}"
          },
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"  
        },
        {
            "src":"/(.*)",
            "dest":"myshop/wsgi.py"
        }
    ]
}